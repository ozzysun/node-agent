{
  "name": "node-agent",
  "version": "1.0.0",
  "description": "build excuable bundle node agent and docker image",
  "main": "src/index.js",
  "bin": "src/index.js",
  "scripts": {
    "start": "nodemon src/index.js",
    "build": "pkg package.json --out-dir output",
    "clean": "rimraf dist/*",
    "lint": "eslint --ext .js src/**/**/*.js tests/**/**/*.js --fix",
    "lint-watch": "esw -w src/**/**/*.js tests/**/**/*.js",
    "test:unit": "nyc mocha -c ./tests/**/*.test.js 2>&1 | tee test.report.log "
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/ozzysun/node-agent.git"
  },
  "keywords": [
    "api",
    "nodejs",
    "restful",
    "graphql"
  ],
  "author": "ozzysun@gmail.com",
  "license": "ISC",
  "homepage": "https://bitbucket.org/ozzysun/node-agent#readme",
  "devDependencies": {
    "eslint": "^6.3.0",
    "eslint-config-node": "^4.0.0",
    "eslint-watch": "^6.0.0",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "require-dir": "^1.2.0",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "sinon": "^7.4.0",
    "supertest": "^4.0.2",
    "yargs": "^13.3.0"
  },
  "pkg": {
    "scripts": [
      "./src/routes/**/**/*.js",
      "./src/graphql/**/**/*.js"
    ],
    "targets": [
      "node10-macos-x64",
      "node10-win-x64"
    ]
  },
  "dependencies": {
    "amqplib": "^0.5.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "consolidate": "^0.15.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-ping": "^1.4.0",
    "express-session": "^1.16.2",
    "ip": "^1.1.5",
    "js-yaml": "^3.13.1",
    "jsonfile": "^5.0.0",
    "jwt-simple": "^0.5.6",
    "multer": "^1.4.2",
    "mysql": "^2.17.1",
    "mysql2": "^1.6.5",
    "request-ip": "^2.1.3",
    "sequelize": "^5.18.4",
    "sequelize-auto": "github:ozzysun/sequelize-auto"
  }
}
